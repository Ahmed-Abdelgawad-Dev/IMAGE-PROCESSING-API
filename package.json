{
  "name": "image-processing-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx tsc && yarn jasmine",
    "start": "nodemon --watch './**/*.ts' --exec 'ts-node' ./src/index.ts",
    "clean": "rimraf build/",
    "copy:screens": "copyfiles -f src/screens/* build/screens",
    "copy:images": "copyfiles -f src/images/*.jpg build/images",
    "build": "yarn clean && npx tsc",
    "prod": "npm run build && nodemon build/index.js",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint . --ext .ts",
    "jasmine": "jasmine"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-validator": "^6.14.0",
    "fs-extra": "^10.0.0",
    "helmet": "^4.6.0",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "morgan": "^1.10.0",
    "sharp": "^0.29.3",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/express-validator": "^3.0.0",
